{
  "name": "file-loader",
  "version": "3.0.1",
  "description": "A file loader module for webpack",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webpack-contrib/file-loader.git"
  },
  "author": {
    "name": "Tobias Koppers @sokra"
  },
  "homepage": "https://github.com/webpack-contrib/file-loader",
